<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td>
        <a href="/update-data?id=<%= users[i]._id%>" class="btn_update border-shadow update">
            <span class="text-gradient">Edit</span>
        </a>
        <a href="#" class="btn_delete border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient">Delete</span>
        </a>
    </td>
</tr>
<% } %>